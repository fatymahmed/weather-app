{
    "env": {
        "browser": true,
        "es6": true
    },
    "extends": [
        "airbnb-base"
    ],
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaVersion": 2018,
        "sourceType": "module"
    },
    "rules": {
        "no-tabs": 0,
        "import/no-cycle": 0,
        "no-alert": 0,
        "no-unused-vars": 0,
        "import/prefer-default-export": "off"
    }
}